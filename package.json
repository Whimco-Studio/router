{
	"name": "@rbxts/router",
	"version": "1.0.17",
	"main": "out/init.lua",
	"scripts": {
		"build": "rbxts-build build",
		"watch": "rbxts-build watch",
		"prepublishOnly": "npm run build",
		"compile": "rbxts-build compile",
		"open": "rbxts-build open",
		"start": "rbxts-build start",
		"stop": "rbxts-build stop",
		"sync": "rbxts-build sync"
	},
	"rbxts-build": {
		"rbxtscArgs": [
			"--verbose",
			"--optimizedLoops"
		],
		"rojoBuildArgs": [
			"--output",
			"game.rbxl"
		],
		"syncLocation": "src/types/services.d.ts",
		"dev": false,
		"wslUseExe": false,
		"watchOnOpen": true,
		"names": {
			"build": "build",
			"compile": "compile",
			"open": "open",
			"watch": "watch"
		}
	},
	"keywords": [],
	"author": "The_Pr0fessor",
	"license": "ISC",
	"description": "A core library for creating REST APIs in Roblox",
	"types": "out/index.d.ts",
	"files": [
		"out",
		"!**/*.tsbuildinfo"
	],
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@rbxts/compiler-types": "^3.0.0-types.0",
		"@rbxts/types": "^1.0.847",
		"@typescript-eslint/eslint-plugin": "^8.30.1",
		"@typescript-eslint/parser": "^8.30.1",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-prettier": "^5.2.6",
		"eslint-plugin-roblox-ts": "^0.0.36",
		"prettier": "^3.5.3",
		"roblox-ts": "^3.0.0",
		"typescript": "^5.8.3",
		"rbxts-build": "^2.4.0"
	},
	"dependencies": {
		"@rbxts/deepcopy": "^0.1.3",
		"@rbxts/lemon-signal": "^1.2.0",
		"@rbxts/luau-polyfill": "^1.1.3",
		"@rbxts/services": "^1.5.5",
		"@rbxts/t": "^3.2.1"
	}
}
